<html>
  <head>
  <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
  <meta charset="utf-8">
  <title>Enroute, your personalized itinerary</title>
<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
  <style>

      
#menu-toggle{
    display: none;
}
.col-lg-12{
    padding: 0 !important;
}      
  html, body, #map-canvas {
    height: 100%;
    width: 100%;
    margin: 0px;
    padding: 0px;
    float: right;
    overflow: visible;
    }
    .gmnoprint{
        visibility: hidden;
    }
      
    .profile-photo{
        display: none;
    }
<!-- NAVBAR CSS --> 
    #wrapper {
    padding-left: 0;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled {
    padding-left: 250px;
}

#sidebar-wrapper {
    z-index: 1000;
    position: fixed;
    left: 250px;
    width: 0;
    height: 100%;
    margin-left: -250px;
    overflow-y: auto;
   background: #2b333b;
    -webkit-transition: all 0.5s ease;
    -moz-transition: all 0.5s ease;
    -o-transition: all 0.5s ease;
    transition: all 0.5s ease;
}

#wrapper.toggled #sidebar-wrapper {
    width: 250px;
}

#page-content-wrapper {
    width: 100%;
    position: absolute;
}

#wrapper.toggled #page-content-wrapper {
    position: absolute;
    margin-right: -250px;
}

/* Sidebar Styles */

.sidebar-nav {
    position: absolute;
    top: 0;
    width: 250px;
    margin: 0;
    padding: 0;
    list-style: none;
    background-color: #2b333b;
    box-shadow: 10px -10px #091a2c;
    max-height: 63.5%;
    overflow-y: scroll;
    display: inline-block;
}

.sidebar-nav li {
    font-family: sans-serif !important;
    font-size: 80%;
}

a>p{
    padding-top: 5px;
    padding-left: 13px;
    padding-right: 13px;
    padding-bottom: 5px;
}

.sidebar-nav li a {
    padding-left: 0 !important;
    display: block;
    text-decoration: none;
    color: #cbe9ff;
}

.sidebar-nav li a:hover {
    text-decoration: none;
    color: #fff;
    background: rgba(255,255,255,0.2);
}

.sidebar-nav li a:active,
.sidebar-nav li a:focus {
    text-decoration: none;
}

.sidebar-nav > .sidebar-brand {
    height: 65px;
    font-size: 18px;
    line-height: 60px;
        padding-left: 12px;
    color: #f5f5f5;
}

.sidebar-nav > .sidebar-brand a {
    color: #f5f5f5;
    padding-left: 12px;
}

.sidebar-nav > .sidebar-brand a:hover {
    color: #fff;
    background: none;
}

@media(min-width:768px) {
    #wrapper {
        padding-left: 250px;
    }

    #wrapper.toggled {
        padding-left: 0;
    }

    #sidebar-wrapper {
        width: 250px;
        display: inline-block;
        
    }

    #wrapper.toggled #sidebar-wrapper {
        width: 0;
    }

    #page-content-wrapper {
        position: relative;
    }

    #wrapper.toggled #page-content-wrapper {
        position: relative;
        margin-right: 0;
    }
}
  </style>
    </head>
<body>
<div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav" style= "position: relative;">
                <li class="sidebar-brand">
                    <a href="#">
                        ENROUTE
                    </a>
                </li>  
                {{#each f_data}}
                <li>
                    <a href="#"><p><span style = "font-weight: bold;">{{this.name }}</span><br />{{ this.region }} <br />{{this.tel}} <br />{{ this.address }}</p></a>
                </li>
                {{/each}}


            </ul>
            
            <div src = "sampleImg" style = "position: absolute; margin-bottom: 20px;text-decoration: none;">
                    <img src = "http://cache.graphicslib.viator.com/graphicslib/thumbs674x446/2050/SITours/eiffel-tower-paris-moulin-rouge-show-and-seine-river-cruise-in-paris-150305.jpg" style = "width: 100%; height: 150px%; margin-bottom: 10px;">
                    <a style = "padding-left: 10px; color: white;">Eiffel Tower<a/>
                        <img src = "http://fifthgroup.com/wp-content/uploads/2014/06/five-stars.png" style = "width: 50px; height: 10px; float: right; padding-right: 10px; top: 5px;">
                        <p style = "float: left; padding-left: 10px; color: white;">The Eiffel Tower is dope as fuck</p>
            </div>
        </div>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->
        <div id="page-content-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div id="map-canvas"></div>
                        <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Toggle Menu</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->
    </div>
</body>
        <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true"></script>
    <script src='https://code.jquery.com/jquery-2.1.3.min.js'></script> 
    <script src = "js/map.js"></script>
</html>
